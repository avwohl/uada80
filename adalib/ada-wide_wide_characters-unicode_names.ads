-- Ada.Wide_Wide_Characters.Unicode_Names for Z80
-- Unicode character names (stub)

package Ada.Wide_Wide_Characters.Unicode_Names is
   pragma Pure;

   -- Stub version for Z80 with basic ASCII control characters

   Nul                  : constant Wide_Wide_Character := Wide_Wide_Character'Val (0);
   Start_Of_Heading     : constant Wide_Wide_Character := Wide_Wide_Character'Val (1);
   Start_Of_Text        : constant Wide_Wide_Character := Wide_Wide_Character'Val (2);
   End_Of_Text          : constant Wide_Wide_Character := Wide_Wide_Character'Val (3);
   End_Of_Transmission  : constant Wide_Wide_Character := Wide_Wide_Character'Val (4);
   Enquiry              : constant Wide_Wide_Character := Wide_Wide_Character'Val (5);
   Acknowledge          : constant Wide_Wide_Character := Wide_Wide_Character'Val (6);
   Bell                 : constant Wide_Wide_Character := Wide_Wide_Character'Val (7);
   Backspace            : constant Wide_Wide_Character := Wide_Wide_Character'Val (8);
   Character_Tabulation : constant Wide_Wide_Character := Wide_Wide_Character'Val (9);
   Line_Feed            : constant Wide_Wide_Character := Wide_Wide_Character'Val (10);
   Line_Tabulation      : constant Wide_Wide_Character := Wide_Wide_Character'Val (11);
   Form_Feed            : constant Wide_Wide_Character := Wide_Wide_Character'Val (12);
   Carriage_Return      : constant Wide_Wide_Character := Wide_Wide_Character'Val (13);
   Space                : constant Wide_Wide_Character := Wide_Wide_Character'Val (32);
   Delete               : constant Wide_Wide_Character := Wide_Wide_Character'Val (127);

end Ada.Wide_Wide_Characters.Unicode_Names;
